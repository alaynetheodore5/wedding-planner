@model Wedding

<div class="jumbotron text-center">
    <h1>Plan Your Wedding:</h1>
    <a href="/logout">Logout</a>
    <div class="text-left">
        <form class="col-6" action="/createwedding" method="post">
            <div class="form-group">
                <label asp-for="WedderOne">Wedder One:</label>
                <input asp-for="WedderOne" class="form-control">
                <span class="text-danger" asp-validation-for="WedderOne"></span>
            </div>
            <div class="form-group">
                <label asp-for="WedderTwo">Wedder Two:</label>
                <input asp-for="WedderTwo" class="form-control">
                <span class="text-danger" asp-validation-for="WedderTwo"></span>
            </div>
            <div class="form-group">
                <label asp-for="WeddingDate">Wedding Date:</label>
                <input asp-for="WeddingDate" class="form-control">
                <span class="text-danger" asp-validation-for="WeddingDate"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address">Wedding Address:</label>
                <input asp-for="Address" class="form-control">
                <span class="text-danger" asp-validation-for="Address"></span>
            </div>
            <input class="btn btn-info" type="submit" value="Create Wedding">
        </form>
    </div>
</div>